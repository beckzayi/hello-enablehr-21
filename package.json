{
  "name": "enablehr-api-documentation-site",
  "private": true,
  "description": "enableHR API documentation site via Gatsby",
  "version": "0.1.0",
  "license": "BSD",
  "scripts": {
    "build": "gatsby build",
    "develop": "gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,md}\"",
    "start": "npm run develop -p 9070",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "eslint": "eslint --config=./.eslintrc.js --format=stylish --color --fix .",
    "prettify": "prettier --write src/"
  },
  "dependencies": {
    "@mdx-js/mdx": "^1.6.22",
    "@mdx-js/react": "^1.6.22",
    "bootstrap": "^5.1.1",
    "gatsby": "^3.13.0",
    "gatsby-plugin-force-trailing-slashes": "^1.0.5",
    "gatsby-plugin-mdx": "^2.13.0",
    "gatsby-plugin-react-helmet": "^4.13.0",
    "gatsby-plugin-sass": "^4.13.0",
    "gatsby-source-filesystem": "^3.13.0",
    "graphiql": "^1.4.2",
    "node-sass": "^6.0.1",
    "openapi-extract": "^1.1.0",
    "openapi-sampler": "^1.1.0",
    "openapi-snippet": "^0.12.1",
    "prism-react-renderer": "^1.2.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-helmet": "^6.1.0",
    "react-markdown": "^7.0.1",
    "swagger-parser": "^10.0.3"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.15.4",
    "@popperjs/core": "^2.10.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^7.0.2",
    "jquery": "^3.6.0",
    "lint-staged": "^11.1.2",
    "popper": "^1.0.1",
    "prettier": "2.4.0",
    "process": "^0.11.10",
    "stream": "0.0.2",
    "stream-browserify": "3.0.0",
    "util": "^0.12.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts}": [
      "npm run prettify",
      "npm run eslint"
    ]
  }
}
